<h1>Editing kennel</h1>

<% form_for(@kennel) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :kennel_name %><br />
    <%= f.text_field :kennel_name %>
  </p>
  <p>
    <%= f.label :owner_name %><br />
    <%= f.text_field :owner_name %>
  </p>
  <p>
    <%= f.label :phone %><br />
    <%= f.text_field :phone %>
  </p>
  <p>
    <%= f.label :location %><br />
    <%= f.text_field :location %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :homepage %><br />
    <%= f.text_field :homepage %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @kennel %> |
<%= link_to 'Back', kennels_path %>